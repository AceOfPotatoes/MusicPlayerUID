<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<AnchorPane minHeight="600.0" minWidth="600.0" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unical.sadstudents.mediaplayeruid.controller.MainController">
   <children>
      <BorderPane fx:id="myBorderPane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <left>
            <VBox maxWidth="-Infinity" prefWidth="270.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Label alignment="CENTER" graphicTextGap="15.0" prefWidth="250.0" text="MediaPlayer UID">
                     <graphic>
                        <FontIcon iconLiteral="fa-play-circle-o" iconSize="23" />
                     </graphic>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Label>
                  <TextField focusTraversable="false" maxWidth="250.0" prefWidth="250.0" promptText="Search" />
                  <ToolBar fx:id="toolbarMenu" minHeight="190.0" orientation="VERTICAL" prefHeight="190.0" prefWidth="255.0" style="-fx-background-color: transparent;" VBox.vgrow="ALWAYS">
                    <items>
                      <Button alignment="TOP_LEFT" graphicTextGap="10.0" mnemonicParsing="false" onAction="#onHome" prefWidth="245.0" style="-fx-background-color: transparent;" text="Home">
                           <graphic>
                              <FontIcon iconLiteral="fa-home" iconSize="19" />
                           </graphic>
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                        <Button alignment="TOP_LEFT" graphicTextGap="10.0" layoutX="10.0" layoutY="13.0" mnemonicParsing="false" onAction="#onMusicLibrary" prefWidth="245.0" style="-fx-background-color: transparent;" text="Music Library">
                           <graphic>
                              <FontIcon iconLiteral="fa-music" iconSize="19" />
                           </graphic>
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                        <Button alignment="TOP_LEFT" graphicTextGap="10.0" layoutX="10.0" layoutY="39.0" mnemonicParsing="false" onAction="#onVideoLibrary" prefWidth="245.0" style="-fx-background-color: transparent;" text="Video Library">
                           <graphic>
                              <FontIcon iconLiteral="fa-film" iconSize="19" />
                           </graphic>
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                        <Separator prefWidth="200.0" />
                        <Button alignment="TOP_LEFT" graphicTextGap="10.0" layoutX="10.0" layoutY="78.0" mnemonicParsing="false" onAction="#onPlayQueue" prefWidth="245.0" style="-fx-background-color: transparent;" text="Play Queue">
                           <graphic>
                              <FontIcon iconLiteral="fa-play" iconSize="19" />
                           </graphic>
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                        <Button alignment="TOP_LEFT" graphicTextGap="10.0" layoutX="10.0" layoutY="78.0" mnemonicParsing="false" onAction="#onPlayLists" prefWidth="245.0" style="-fx-background-color: transparent;" text="Playlists">
                           <graphic>
                              <FontIcon iconLiteral="fa-list-ul" iconSize="19" />
                           </graphic>
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                    </items>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </ToolBar>
                  <Button alignment="BOTTOM_LEFT" graphicTextGap="10.0" mnemonicParsing="false" onAction="#onSettings" prefHeight="30.0" prefWidth="240.0" style="-fx-background-color: transparent;" text="Settings">
                     <graphic>
                        <FontIcon iconLiteral="fa-cog" iconSize="19" />
                     </graphic>
                     <font>
                        <Font size="14.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </Button>
               </children>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </VBox>
         </left>
         <bottom>
            <VBox alignment="CENTER" prefWidth="1200.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" spacing="10.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label fx:id="timeMediaPlayed" maxWidth="52.0" minWidth="52.0">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <ProgressBar fx:id="progressBar" prefWidth="1052.0" progress="0.0" HBox.hgrow="ALWAYS" />
                        <Label fx:id="durationMediaPlayed" maxWidth="52.0" minWidth="52.0">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets left="10.0" right="10.0" />
                     </padding>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" prefHeight="23.0" prefWidth="1200.0" spacing="10.0" VBox.vgrow="ALWAYS">
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="250.0">
                           <children>
                              <ImageView fitHeight="56.0" fitWidth="68.0" pickOnBounds="true" preserveRatio="true" />
                              <VBox alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="180.0">
                                 <children>
                                    <Label fx:id="nameMediaPlayed" prefWidth="180.0" text="Label">
                                       <font>
                                          <Font size="14.0" />
                                       </font>
                                    </Label>
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="5.0" HBox.hgrow="ALWAYS">
                           <children>
                              <Button mnemonicParsing="false" onAction="#onShuffle" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-random" iconSize="15" />
                                 </graphic>
                              </Button>
                              <Button layoutX="312.0" layoutY="35.0" mnemonicParsing="false" onAction="#onPrevious" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-fast-backward" iconSize="15" />
                                 </graphic>
                              </Button>
                              <Button layoutX="309.0" layoutY="36.0" mnemonicParsing="false" onAction="#onSkipBack" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-backward" iconSize="15" />
                                 </graphic>
                              </Button>
                              <Button layoutX="284.0" layoutY="35.0" mnemonicParsing="false" onAction="#onPlayPause" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon fx:id="iconPlayPause" iconLiteral="fa-play" iconSize="25" />
                                 </graphic>
                              </Button>
                              <Button layoutX="347.0" layoutY="36.0" mnemonicParsing="false" onAction="#onSkipForward" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-forward" iconSize="15" />
                                 </graphic>
                              </Button>
                              <Button layoutX="256.0" layoutY="35.0" mnemonicParsing="false" onAction="#onNext" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-fast-forward" iconSize="15" />
                                 </graphic>
                              </Button>
                              <Button layoutX="228.0" layoutY="35.0" mnemonicParsing="false" onAction="#onRepeat" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-repeat" iconSize="15" />
                                 </graphic>
                              </Button>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" layoutX="280.0" layoutY="10.0" prefHeight="100.0" prefWidth="250.0" spacing="5.0">
                           <children>
                              <Slider fx:id="volumeSlider" prefHeight="16.0" prefWidth="87.0" value="50.0" HBox.hgrow="ALWAYS">
                                 <padding>
                                    <Insets top="2.0" />
                                 </padding>
                              </Slider>
                              <Button mnemonicParsing="false" onAction="#onVolume" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-volume-up" iconSize="15" />
                                 </graphic>
                              </Button>
                              <Button layoutX="240.0" layoutY="36.0" mnemonicParsing="false" onAction="#onScreenMode" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-expand" iconSize="15" />
                                 </graphic>
                              </Button>
                              <Button layoutX="191.0" layoutY="36.0" mnemonicParsing="false" onAction="#onSpeedPlay" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-dashboard" iconSize="15" />
                                 </graphic>
                              </Button>
                              <Button layoutX="200.0" layoutY="36.0" mnemonicParsing="false" onAction="#onEquilizer" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-sliders" iconSize="15" />
                                 </graphic>
                              </Button>
                              <Button layoutX="240.0" layoutY="36.0" mnemonicParsing="false" onAction="#onProperties" prefWidth="20.0" style="-fx-background-color: transparent;">
                                 <graphic>
                                    <FontIcon iconLiteral="fa-info-circle" iconSize="15" />
                                 </graphic>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                     <padding>
                        <Insets left="10.0" right="10.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
